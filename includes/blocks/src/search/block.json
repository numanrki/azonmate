{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "azonmate/search",
	"version": "1.0.0",
	"title": "AzonMate Product Search",
	"category": "widgets",
	"icon": "search",
	"description": "Search and insert Amazon products directly in the editor.",
	"keywords": ["amazon", "search", "product", "azonmate", "affiliate"],
	"supports": {
		"html": false,
		"multiple": true
	},
	"textdomain": "azonmate",
	"attributes": {
		"asin": {
			"type": "string",
			"default": ""
		},
		"displayType": {
			"type": "string",
			"default": "box"
		},
		"template": {
			"type": "string",
			"default": "default"
		}
	},
	"editorScript": "file:./index.js",
	"render": "file:./render.php"
}
